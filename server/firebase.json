{
  "hosting": {
    "target": "cloud-run",
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "ingitdb-server",
          "region": "europe-west3"
        }
      }
    ],
    "cleanUrls": true,
    "trailingSlashBehavior": "REMOVE"
  }
}
